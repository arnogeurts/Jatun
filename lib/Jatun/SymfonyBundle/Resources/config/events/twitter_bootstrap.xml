<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="jatun.event.modal_open.class">Jatun\Event\TwitterBootstrap\ModalOpenEvent</parameter>
        <parameter key="jatun.event.modal_close.class">Jatun\Event\TwitterBootstrap\ModalCloseEvent</parameter>
    </parameters>

    <services>
        <service id="jatun.event.modal_open" class="%jatun.event.modal_open.class%" public="false">
            <tag name="jatun.event" />
        </service>
        
        <service id="jatun.event.modal_close" class="%jatun.event.modal_close.class%" public="false">
            <tag name="jatun.event" />
        </service>
    </services>
</container>
